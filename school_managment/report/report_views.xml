<odoo>
<record id="student_hostel_receipt" model="ir.actions.report">
        <field name="name">Print receipt</field>
        <field name="model">hostel.admission</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">school_managment.print_receipt</field>
        <field name="report_file">school_managment.print_receipt</field>
        <field name="binding_model_id" ref="model_hostel_admission"/>
        <field name="binding_type">report</field>
    </record>


